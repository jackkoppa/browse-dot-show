{
  "name": "@browse-dot-show/search-orchestrator-lambda",
  "private": true,
  "version": "0.0.1",
  "description": "Search orchestrator that coordinates searches across multiple shard Lambdas",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build:prod": "dotenvx run -f ../../../.env.lambda-prod-build -- rolldown -c rolldown.config.ts && pnpm __prepare-for-aws",
    "dev:local": "tsx dev-server.ts",
    "dev:health-check": "tsx health-check.ts",
    "test": "vitest run",
    "__prepare-for-aws": "tsx ../../../scripts/pnpm-deploy-with-versions-fix.ts @browse-dot-show/search-orchestrator-lambda"
  },
  "dependencies": {
    "@aws-sdk/client-lambda": "catalog:"
  },
  "devDependencies": {
    "@browse-dot-show/constants": "workspace:*",
    "@browse-dot-show/s3": "workspace:*",
    "@browse-dot-show/logging": "workspace:*",
    "@browse-dot-show/types": "workspace:*",
    "rolldown": "catalog:",
    "typescript": "catalog:",
    "tsx": "^4.7.0",
    "vitest": "catalog:"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
